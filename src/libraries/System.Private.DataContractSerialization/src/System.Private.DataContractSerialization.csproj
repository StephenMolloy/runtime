<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>$(NetCoreAppCurrent)</TargetFramework>
    <NoWarn>$(NoWarn);1634;1691;649</NoWarn>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <!-- Too much private reflection. Do not bother with trimming -->
    <ILLinkTrimAssembly>false</ILLinkTrimAssembly>
  </PropertyGroup>

  <PropertyGroup>
    <RuntimeSerializationSources>System\Runtime\Serialization</RuntimeSerializationSources>
    <JsonSources>System\Runtime\Serialization\Json</JsonSources>
    <XmlSources>System\Xml</XmlSources>
    <TextSources>System\Text</TextSources>
  </PropertyGroup>

  <PropertyGroup>
    <!--
    smolloy_codedom_partial_internal => same as existing - only CodeTypeReference et al - Serializers/internal
    smolloy_codedom_stubbed
    smolloy_codedom_full_internalish
    :nothing: ==> old 'CODEDOM', meaning we build/include full public implementation of any codedom class we include. Not what we want.
    -->
    <CodeDomMode>smolloy_codedom_full_internalish</CodeDomMode>

    <!-- Adding Schema Import requires adding the extended surrogate support, which in turn requires choosing a non-status-quo codedom implementation. -->
    <DefineConstants>smolloy_add_json_surrogate;$(CodeDomMode);smolloy_add_ext_surrogate;smolloy_add_schema_import</DefineConstants>
    <!--<DefineConstants>smolloy_keep_codegentrace;smolloy_keep_known_serializable_types;smolloy_keep_kvpadapter;
                         smolloy_add_json_surrogate;smolloy_add_ext_surrogate;smolloy_add_schema_import</DefineConstants>-->

  </PropertyGroup>



